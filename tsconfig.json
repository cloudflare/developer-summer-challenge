{
  "compilerOptions": {
    "strict": true,
    "target": "es2019",
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "allowJs": false,
  },
  "include": [
    "worker",
  ],
  "exclude": [
    "node_modules"
  ]
}
